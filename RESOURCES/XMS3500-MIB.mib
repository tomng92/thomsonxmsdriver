     -------
     -- redResFixedTable definition
     -------

     redResFixedTable OBJECT-TYPE
          SYNTAX       SEQUENCE OF RedResFixedEntry
          MAX-ACCESS   not-accessible
          STATUS       current
          DESCRIPTION  ""
          ::= { redReplaceableResource 1 }

     redResFixedEntry OBJECT-TYPE
          SYNTAX       RedResFixedEntry
          MAX-ACCESS   not-accessible
          STATUS       current
          DESCRIPTION  ""
          INDEX        { redResFixedGroupIndex, redResFixedElementIndex }
          ::= { redResFixedTable 1 }

     RedResFixedEntry ::=
          SEQUENCE {
                     redResFixedGroupIndex
                          Unsigned32,
                     redResFixedElementIndex
                          Unsigned32,
                     redResFixedRedundancyStatus
                          INTEGER,
                     redResFixedFunction
                          INTEGER,
                     redResFixedSwitchMode
                          INTEGER,
                     redResFixedMirroredElementIndex
                          Unsigned32,
                     redResFixedBackupElementIndex
                          Unsigned32,
                     redResFixedSwitchCommand
                          INTEGER,
                     redResFixedRestoreMode
                          INTEGER
                   }

     redResFixedGroupIndex OBJECT-TYPE
          SYNTAX       Unsigned32 
          MAX-ACCESS   read-only
          STATUS       current
          DESCRIPTION  "Reference to the Redundancy Group handling the resource"
          ::= { redResFixedEntry 1 }

     redResFixedElementIndex OBJECT-TYPE
          SYNTAX       Unsigned32 
          MAX-ACCESS   read-only
          STATUS       current
          DESCRIPTION  "Reference to the Element defining the resource (value of the topoElementIndex field of the Element)"
          ::= { redResFixedEntry 2 }

     redResFixedRedundancyStatus OBJECT-TYPE
          SYNTAX       INTEGER {
                                 standby(1),
                                 active(2),
                                 maybeActive(6),
                                 maybeStandby(7)
                               }
          MAX-ACCESS   read-only
          STATUS       current
          DESCRIPTION  "Redundancy status of the resource (Active / Standby / maybeActive / maybeStandby)"
          ::= { redResFixedEntry 3 }

     redResFixedFunction OBJECT-TYPE
          SYNTAX       INTEGER {
                                 nominal(0),
                                 redundant(1)
                               }
          MAX-ACCESS   read-only
          STATUS       current
          DESCRIPTION  "Function of the resource (Nominal / Redundant)"
          ::= { redResFixedEntry 4 }

     redResFixedSwitchMode OBJECT-TYPE
          SYNTAX       INTEGER {
                                 manual(0),
                                 auto(1),
                                 switchBackOnRedundantFault(2),
                                 switchBackOnNominalFaultless(3),
                                 none(4),
                                 auto_controlled_by_xms(5),
                                 auto_controlled_by_switcher(6),
                                 auto_combined(7)
                               }
          MAX-ACCESS   read-only
          STATUS       current
          DESCRIPTION  "Switch mode (manual,auto backup, etc.). This field has a meaning only for nominal resources."
          ::= { redResFixedEntry 5 }

     redResFixedMirroredElementIndex OBJECT-TYPE
          SYNTAX       Unsigned32 
          MAX-ACCESS   read-write
          STATUS       current
          DESCRIPTION  "Reference to the resource (in this table) mirrored by this one. A reference to itself if this resource isn't mirroring any other one (this is the case for nominal resources)."
          ::= { redResFixedEntry 6 }

     redResFixedBackupElementIndex OBJECT-TYPE
          SYNTAX       Unsigned32 
          MAX-ACCESS   read-write
          STATUS       current
          DESCRIPTION  "Reference to the backup resource (in this table) selected to perform the switch command (by default the mirroring element, if any exists). Setting a '0' forces the XMS to choose itself the available redundant, in n+p schemes (but if none is available, the switch is not performed)."
          ::= { redResFixedEntry 7 }

     redResFixedSwitchCommand OBJECT-TYPE
          SYNTAX       INTEGER {
                                 switch(1),
                                 reactivate(2)
                               }
          MAX-ACCESS   read-write
          STATUS       current
          DESCRIPTION  "Switch/reactivate command. This command can be applied only on nominal resources."
          ::= { redResFixedEntry 8 }

     redResFixedRestoreMode OBJECT-TYPE
          SYNTAX       INTEGER {
                                 manual(1),
                                 switchBackOnNominalFaultless(2),
                                 switchBackOnRedundantFault(3),
                                 switchBackOnNominalOutputFaultless(4),
                                 switchBackOnRedundantOutputFault(5)
                               }
          MAX-ACCESS   read-only
          STATUS       current
          DESCRIPTION  "Restore mode. This field has a meaning only for nominal resources."
          ::= { redResFixedEntry 9 }


