          ::= { redResSwitchableSourceEntry 2 }

     redResSwitchSourceRedundancyStatus OBJECT-TYPE
          SYNTAX       INTEGER {
                                 standby(1),
                                 active(2),
                                 maybeActive(6),
                                 maybeStandby(7)
                               }
          MAX-ACCESS   read-only
          STATUS       current
          DESCRIPTION  "Redundancy status of the source (Active / Standby=replaced for a nominal source / maybeActive / maybeStandby)"
