     -------
     -- genManagerTable definition
     -------

     genManagerTable OBJECT-TYPE
          SYNTAX       SEQUENCE OF GenManagerEntry
          MAX-ACCESS   not-accessible
          STATUS       current
          DESCRIPTION  ""
          ::= { general 1 }

     genManagerEntry OBJECT-TYPE
          SYNTAX       GenManagerEntry
          MAX-ACCESS   not-accessible
          STATUS       current
          DESCRIPTION  ""
          INDEX        { genManagerIpAddress, genManagerPort }
          ::= { genManagerTable 1 }

     GenManagerEntry ::=
          SEQUENCE {
                     genManagerIpAddress
                          IpAddress,
                     genManagerPort
                          INTEGER,
                     genManagerTrapEnable
                          Boolean,
                     genManagerTrapTypeSelection
                          INTEGER,
                     genManagerEventTrapSeverityFilter
                          INTEGER,
                     genManagerRowStatus
                          RowStatus
                   }

     genManagerIpAddress OBJECT-TYPE
          SYNTAX       IpAddress 
          MAX-ACCESS   read-write
          STATUS       current
          DESCRIPTION  "The manager IP address"
          ::= { genManagerEntry 1 }

     genManagerPort OBJECT-TYPE
          SYNTAX       INTEGER 
          MAX-ACCESS   read-write
          STATUS       current
          DESCRIPTION  "The manager port"
          ::= { genManagerEntry 2 }

     genManagerTrapEnable OBJECT-TYPE
          SYNTAX       Boolean 
          MAX-ACCESS   read-write
          STATUS       current
          DESCRIPTION  "Enable or not sending traps of any type towards this manager"
          ::= { genManagerEntry 3 }

     genManagerTrapTypeSelection OBJECT-TYPE
          SYNTAX       INTEGER 
          MAX-ACCESS   read-write
          STATUS       current
          DESCRIPTION  "Specify the types of trap to be sent (1 bit per type, value 0 = disabled). bit0 : genTrapAlarm, bit1: genTrapTopology, bit2: genTrapConfigXxx, bit3: genTrapRedundancy, bit4 : genTrapEvent"
          ::= { genManagerEntry 4 }

     genManagerEventTrapSeverityFilter OBJECT-TYPE
          SYNTAX       INTEGER {
                                 allSeverity(2),
                                 severityWarning(3),
                                 severityMinor(4),
                                 severityMajor(5),
                                 severityCritical(6)
                               }
          MAX-ACCESS   read-write
          STATUS       current
          DESCRIPTION  "Only event traps whose severity is higher or equal this value will be sent, and associated events of severity cleared"
          ::= { genManagerEntry 5 }

     genManagerRowStatus OBJECT-TYPE
          SYNTAX       RowStatus 
          MAX-ACCESS   read-create
          STATUS       current
          DESCRIPTION  "Status of row"
          ::= { genManagerEntry 6 }


