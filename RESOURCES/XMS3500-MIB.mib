     -------
     -- redGroupTable definition
     -------

     redGroupTable OBJECT-TYPE
          SYNTAX       SEQUENCE OF RedGroupEntry
          MAX-ACCESS   not-accessible
          STATUS       current
          DESCRIPTION  ""
          ::= { redundancy 1 }

     redGroupEntry OBJECT-TYPE
          SYNTAX       RedGroupEntry
          MAX-ACCESS   not-accessible
          STATUS       current
          DESCRIPTION  ""
          INDEX        { redGroupIndex }
          ::= { redGroupTable 1 }

     RedGroupEntry ::=
          SEQUENCE {
                     redGroupIndex
                          Unsigned32,
                     redGroupName
                          DisplayString,
                     redGroupType
                          INTEGER
                   }

     redGroupIndex OBJECT-TYPE
          SYNTAX       Unsigned32 
          MAX-ACCESS   read-only
          STATUS       current
          DESCRIPTION  "Index of a Redundancy Group in the table"
          ::= { redGroupEntry 1 }

     redGroupName OBJECT-TYPE
          SYNTAX       DisplayString (SIZE(0..255))
          MAX-ACCESS   read-only
